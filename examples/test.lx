@module "main"

@use "memory" as mem

pub const main = fn () int {
    let p: *void = mem::alligned_alloc(64, 100);
    defer { free(p); }

    output("Pointer: ", *p, "\n");
    output("Aligned? ");

    if ((cast<int>(p) / 64) * 64 == cast<int>(p)) {
        output("yes\n");
    } else {
        output("no\n");
    }

    return 0;
}
