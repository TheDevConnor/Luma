@module "main"

@use "stdstr" as s 

pub const main = fn () int {
    let x: *char = "Hello, World!";
    let y: *char = "Hello, There!";
    let dest: *char = cast<*char>(alloc(100));
    let z: *char = cast<*char>(alloc(100));
    defer { free(dest); free(z); }

    output(s::strcmp(x, y), "\n");
    output(s::cat(dest, x, y), "\n");
    s::n_copy(z, x, 5);
    output(z, "\n");
    output(s::s_char(x, 87), "\n");

    return 0;
}
