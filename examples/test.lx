@module "main"

@use "memory" as mem

pub const main = fn () int {
    let p: *void = mem::aligned_alloc(64, 100);
    defer { mem::aligned_free(p); } 

    output("Pointer: ", cast<int>(p), "\n");  
    output("Aligned? ");

    if ((cast<int>(p) / 64) * 64 == cast<int>(p)) {
        output("yes\n");
    } else {
        output("no\n");
    }

    return 0;
}

