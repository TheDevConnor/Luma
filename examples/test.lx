@module "main"

@use "fib" as fib

const WeekDay = enum {
    Sunday,
    Monday,
    Tuesday,
    Wednesday,
    Thursday,
    Friday,
    Saturday,
};

pub const main = fn () int {
    let x: WeekDay;

    switch (x) {
        WeekDay::Monday, WeekDay::Tuesday, WeekDay::Wednesday, 
        WeekDay::Thursday => {
            output("Here ", fib.fib(x, 0, 1), "\n");
        }
        WeekDay::Friday, WeekDay::Saturday, 
        WeekDay::Sunday => {
            output(x, "\n");
        }
        // _ => output("Weekday was not found.\n");
    }

    return 0;
}
