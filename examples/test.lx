@module "main"

pub const main = fn () int { 
   let x: int = 5;
   let row: int = 0;

   loop [i: int = 0, j: int = 0](i <= x) {
      if (j > 10) {
         j = 0;
         i++;
      } elif (row == 3) {
          row = 0;
          output("\n");
      } else {   
         output(i, "*", j, "=", i * j, " ");
         row++;
         j++;
      }
   }

   return 0; 
}
