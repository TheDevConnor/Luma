@module "main"

@use "stdstr" as s 

pub const main = fn () int {
    let x: *char = "Hello, World!";
    let y: *char = "Hello, There!";
    output(s::strcmp(x, y), "\n");
    output(s::cat(x, y), "\n");

    let z: *char = cast<*char>(alloc(100));
    defer { free(z); }
    s::n_copy(z, x, 5);
    output(z, "\n");

    let msg: *char = "Hello, World!";
    let p: char;
    p = s::s_char(msg, 87);
    output(p, "\n");

    return 0;
}
