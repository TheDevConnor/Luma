@module "main"
@use "math" as math

pub const main = fn () int {
    output("add: ", math.add(3, 4), "\n");
    output("subtract: ", math.subtract(10, 5), "\n");
    output("multiply: ", math.multiply(6, 7), "\n");
    output("divide: ", math.divide(20, 4), "\n");
    return 0;
}

