@module "main"

#returns_ownership
const from_char = fn (c: char) *char {
  let s: *char = cast<*char>(alloc(2 * sizeof<char>));
  s[0] = c;
  s[1] = cast<char>(0);
  return s;
} 

pub const main = fn () int {
    let c: *char = from_char('A');
    defer { free(c); }
    output(c, "\n");
    return 0;
}
